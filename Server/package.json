{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "yarn clean && yarn build-babel && sequelize db:migrate",
    "build-babel": "babel ./src --out-dir build --ignore **/*.test.js ",
    "clean": "rm -rf build && mkdir build",
    "start": "node ./build/index.js",
    "db:reset": "npx sequelize-cli db:drop && yarn db:init && yarn db:seed",
    "db:init": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
