    "firebase-admin": "^9.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mqtt": "^4.2.6",
    "nodemailer": "^6.6.0",
    "pg": "^8.6.0",
